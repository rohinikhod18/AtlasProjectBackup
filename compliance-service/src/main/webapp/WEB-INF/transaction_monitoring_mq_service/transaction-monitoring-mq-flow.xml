<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:int="http://www.springframework.org/schema/integration"
	xmlns:int-http="http://www.springframework.org/schema/integration/http"
	xmlns:util="http://www.springframework.org/schema/util"
	xmlns:task="http://www.springframework.org/schema/task"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd 
http://www.springframework.org/schema/integration 
http://www.springframework.org/schema/integration/spring-integration.xsd 
http://www.springframework.org/schema/task 
http://www.springframework.org/schema/task/spring-task.xsd 
http://www.springframework.org/schema/context 
http://www.springframework.org/schema/context/spring-context.xsd 
http://www.springframework.org/schema/integration/http
http://www.springframework.org/schema/integration/http/spring-integration-http.xsd
http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd">



	<int:channel
		id="PROFILE.TRANSACTION_MONITORING_MQ_RESEND.in.channel" />
	<int:channel id="PUSH.TMMQ.OUT.CHANNEL" />
	<int:channel id="TMMQ.SERVICE.IN.CHANNEL" />

	<int:chain id="tmmq.initChain"
		input-channel="PROFILE.TRANSACTION_MONITORING_MQ_RESEND.in.channel">
		<int:service-activator
			ref="TransactionMonitoringMQDBServiceImpl" id="TMMQ.DB.LOAD"
			method="loadMessageFromDB" />
		<int:router id="tmmq.checkmessageleft"
			default-output-channel="PUSH.TMMQ.OUT.CHANNEL"
			expression="payload.getNoMessageLeft()">
			<int:mapping value="true" channel="PUSH.TMMQ.OUT.CHANNEL" />
			<int:mapping value="false"
				channel="TMMQ.SERVICE.IN.CHANNEL" />
		</int:router>

	</int:chain>

	<int:chain id="tmmq.servicechain"
		input-channel="TMMQ.SERVICE.IN.CHANNEL"
		output-channel="MQ.INTUITIONSTATUS">
		<int:service-activator id="tm.mq.createEvent"
			ref="eventDBServiceImpl" method="createEventTMMQ" />
		<int:transformer ref="tmmqTransformer"
			method="transformRequest" />
		<int:service-activator
			id="TMMQ.TRANSACTIONMONITORING" method="sendRequest">
			<bean
				class="com.currenciesdirect.gtg.compliance.compliancesrv.util.HttpClientPool"
				p:urlBase="#{providerProperties.getEndPointUrl('TRANSACTION_MONITORING_MQ_RESEND')}"
				p:method="POST"
				p:serviceType="#{T(com.currenciesdirect.gtg.compliance.compliancesrv.msg.ServiceTypeEnum).TRANSACTION_MONITORING_MQ_RESEND}"
				p:requestClass="com.currenciesdirect.gtg.compliance.commons.domain.transactionmonitoring.TransactionMonitoringMQServiceRequest"
				p:responseClass="com.currenciesdirect.gtg.compliance.commons.domain.transactionmonitoring.TransactionMonitoringMQServiceResponse" />
		</int:service-activator>
		<int:transformer ref="tmmqTransformer"
			method="transformResponse" />

		<int:service-activator
			id="SAVE.EVENT.SERVICE.TM" ref="eventDBServiceImpl"
			method="createEventSerivceLog">
		</int:service-activator>

		<int:service-activator
			id="TMMQ.Exec.Rules" ref="MQRulesService"
			method="process">
		</int:service-activator>

		<int:service-activator
			ref="TransactionMonitoringMQDBServiceImpl" id="TMMQ.DB.UPDATE"
			method="updateMQStatusToDB" />
			
		<int:service-activator
			ref="TransactionMonitoringMQDBServiceImpl" id="TMMQ.DB.UPDATE.INTUITION.RISK.LEVEL"
			method="updateAccountIntuitionRiskLevel" />
		
			

		<int:service-activator
			id="profile.activitylog.tm" ref="tmactivityDBServiceImpl"
			method="createSTPTransactionMonitoringMQActivityLog" />
		<int:service-activator
			id="fundsin.kafkaTxExecutor"
			ref="kafkaTxExecutorServiceImpl"
			method="executeKafkaTx" />	

		<int:service-activator
			ref="TransactionMonitoringMQDBServiceImpl" id="TMMQ.DB.DELETE"
			method="removeDeliveredRecordFromTMMQTable" />
			
		<int:service-activator
			ref="TransactionMonitoringMQDBServiceImpl" id="TMMQ.DB.STATUS.BROADCAST"
			method="updateStatusInBoardCastQueue" />

<!-- If retry count is 5 then we send alert email to support team -->
		<int:service-activator
			ref="TransactionMonitoringEmailAlertImpl" id="TMMQ.EMAIL.ALERT"
			method="sendEmailAlert" />

	</int:chain>

	<int:chain
		input-channel="MQ.INTUITIONSTATUS">
		<int:router
			default-output-channel="TRANSACTIONMONITORING_BULK_REPROCESS"
			resolution-required="false"
			expression="payload.isIntuitionMQStatusHold()">
			<int:mapping
				value="true"
				channel="TRANSACTIONMONITORINGMQSTATUS" />
			<int:mapping
				value="false"
				channel="TRANSACTIONMONITORING_BULK_REPROCESS" />
		</int:router>
	</int:chain>
	
	<int:chain input-channel="TRANSACTIONMONITORINGMQSTATUS"
	output-channel="TRANSACTIONMONITORING_BULK_REPROCESS"> 
		<int:service-activator
			id="TMMQ.TRANSACTIONMONITORING.RESEND" method="sendRequest">
			<bean
				class="com.currenciesdirect.gtg.compliance.compliancesrv.util.HttpClientPool"
				p:urlBase="#{providerProperties.getEndPointUrl('TRANSACTION_MONITORING_MQ_RESEND')}"
				p:method="POST"
				p:serviceType="#{T(com.currenciesdirect.gtg.compliance.compliancesrv.msg.ServiceTypeEnum).TRANSACTION_MONITORING_MQ_RESEND}"
				p:requestClass="com.currenciesdirect.gtg.compliance.commons.domain.transactionmonitoring.TransactionMonitoringMQServiceRequest"
				p:responseClass="com.currenciesdirect.gtg.compliance.commons.domain.transactionmonitoring.TransactionMonitoringMQServiceResponse" />
		</int:service-activator>
	</int:chain>

	<int:chain input-channel="TRANSACTIONMONITORING_BULK_REPROCESS"
	output-channel="PUSH.TMMQ.OUT.CHANNEL">
	
		<int:service-activator
			ref="TMMQBulkProcessingService" id="TMMQ.BULK.PROCESS"
			method="bulkProcessing" />
			
	</int:chain>

	<int:chain id="tmmq.responsechain"
		input-channel="PUSH.TMMQ.OUT.CHANNEL" output-channel="nullChannel">
		<int:service-activator
			ref="transactionMonitoringMqService" method="handleResponse" />
	</int:chain>
	
	<bean id="TransactionMonitoringMQDBServiceImpl"
		class="com.currenciesdirect.gtg.compliance.compliancesrv.dbport.TransactionMonitoringMQDBServiceImpl" />
		
	<bean id="eventDBServiceImpl"
		class="com.currenciesdirect.gtg.compliance.compliancesrv.dbport.EventDBServiceImpl" />
		
	<bean id="tmmqTransformer"
		class="com.currenciesdirect.gtg.compliance.compliancesrv.transformer.reg.TransactionMonitoringMQTransformer" />

	<bean id="transactionMonitoringMqService"
		class="com.currenciesdirect.gtg.compliance.compliancesrv.core.transactionmonitoringmq.TransactionMonitoringMQService" />

	<bean id="tmactivityDBServiceImpl"
		class="com.currenciesdirect.gtg.compliance.compliancesrv.dbport.ActivityDBServiceImpl" />
		
	<bean id="TransactionMonitoringEmailAlertImpl"
		class="com.currenciesdirect.gtg.compliance.compliancesrv.tmemailalert.TransactionMonitoringEmailAlertImpl" />
		
	<bean id="MQRulesService"
		class="com.currenciesdirect.gtg.compliance.compliancesrv.core.rules.MQPaymentRuleService" />
		
	<bean id="TMMQBulkProcessingService"
		class="com.currenciesdirect.gtg.compliance.compliancesrv.core.transactionmonitoringmq.TMMQBulkProcessingService" />

</beans>