<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xmlns:int="http://www.springframework.org/schema/integration"
	xmlns:int-http="http://www.springframework.org/schema/integration/http"
	xmlns:util="http://www.springframework.org/schema/util" xmlns:task="http://www.springframework.org/schema/task"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans

http://www.springframework.org/schema/beans/spring-beans.xsd 
http://www.springframework.org/schema/integration 
http://www.springframework.org/schema/integration/spring-integration.xsd 
http://www.springframework.org/schema/task 
http://www.springframework.org/schema/task/spring-task.xsd 
http://www.springframework.org/schema/context 
http://www.springframework.org/schema/context/spring-context.xsd 
http://www.springframework.org/schema/integration/http
http://www.springframework.org/schema/integration/http/spring-integration-http.xsd
http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd">

	<context:component-scan
		base-package="com.currenciesdirect.gtg.compliance.compliancesrv" />
	<int:channel id="FUNDSOUT.BLACKLIST_RESEND.in.channel"/>
	<int:channel id="FUNDSOUT.BLACKLIST_RESEND.process"/>
	<int:channel id="FUNDSOUT.BLACKLISTRESEND.out.channel"/>
	<int:channel id="FUNDSOUT.BLACKLIST.RESEND.CHANNEL"/>
	<int:channel id="FUNDSOUT.BLACKLISTRESEND.CREATE.EVENT"/>
	<int:channel id="FUNDSOUT.BLACKLISTRESEND.REQUEST.SENDER"/>
	<int:channel id="FUNDSOUT.BLACKLISTRESEND.RESPONSE"/>
	<int:channel id="FUNDSOUT.BLACKLISTRESEND.ACTIVITY.LOG"/>
	<int:channel id="FUNDSOUT.BLACKLIST_RESEND.ERROR.CHANNEL"/>
	
	<int:chain id="resendFundsOutBlacklistService" 
		input-channel="FUNDSOUT.BLACKLIST_RESEND.in.channel">
		<int:service-activator 
			ref="fundsout.MsgValidator"
			method="validateBlacklistResendReuqest" />	
		<int:router
			id="fundsoutBlacklistResendValidation"
			expression="payload.getValidationResult()">
			<int:mapping
				value="FAIL"
				channel="FUNDSOUT.BLACKLISTRESEND.RESPONSE" />
			<int:mapping
				value="SUCCESS"
				channel="FUNDSOUT.BLACKLIST_RESEND.process" />
		</int:router>
	</int:chain>
	
	<int:chain id="fundsOutblacklistDataEnrich" input-channel="FUNDSOUT.BLACKLIST_RESEND.process" output-channel="FUNDSOUT.BLACKLISTRESEND.CREATE.EVENT">
		<int:service-activator
			id="FUNDSOUT.BlacklistResend.ENRICH"
			ref="fundsout.DataEnrich"
			method="enrichDataForBlacklistRecheck" />
	</int:chain>
	
	<int:chain id="fundsOutblacklistEventCreater" input-channel="FUNDSOUT.BLACKLISTRESEND.CREATE.EVENT" output-channel="FUNDSOUT.BLACKLISTRESEND.REQUEST.SENDER">
		<int:service-activator
			id="fundsout.BLACKLISTRESEND.event"
			method="createEvent" ref="eventDBServiceImpl">
		</int:service-activator>
	</int:chain>
	
		<int:chain id="fundsOutblacklistRequestSender" input-channel="FUNDSOUT.BLACKLISTRESEND.REQUEST.SENDER" output-channel="FUNDSOUT.BLACKLISTRESEND.out.channel">
		<int:transformer
			ref="fundsout.blacklistTransformer"
			method="transformBlacklistResendRequest" />
		<int:service-activator
			id="fundsout.BLACKLIST.RESEND"
			method="sendRequest">
			<bean
				class="com.currenciesdirect.gtg.compliance.compliancesrv.util.HttpClientPool"
				p:urlBase="#{providerProperties.getEndPointUrl('INTERNAL_RULE_SERVICE')}"
				p:method="POST"
				p:serviceType="#{T(com.currenciesdirect.gtg.compliance.compliancesrv.msg.ServiceTypeEnum).INTERNAL_RULE_SERVICE}"
				p:requestClass="com.currenciesdirect.gtg.compliance.commons.domain.internalruleservice.InternalServiceRequest"
				p:responseClass="com.currenciesdirect.gtg.compliance.commons.domain.internalruleservice.InternalServiceResponse" />
		</int:service-activator>
		<int:transformer
			ref="fundsout.blacklistTransformer"
			method="transformBlacklistResendResponse" />
			<int:filter discard-channel="FUNDSOUT.BLACKLIST_RESEND.ERROR.CHANNEL" expression="!payload.isFailed()" />
	</int:chain>
	
	 <int:chain input-channel="FUNDSOUT.BLACKLISTRESEND.out.channel" output-channel="FUNDSOUT.BLACKLISTRESEND.ACTIVITY.LOG">
		<int:service-activator id="fundsout.BLACKLISTRESEND.SAVE.EVENT.SERVICE"
			method="createEventSerivceLog" ref= "eventDBServiceImpl">
		</int:service-activator>
		<int:filter  discard-channel="FUNDSOUT.BLACKLIST_RESEND.ERROR.CHANNEL" expression="!payload.isFailed()" />
	</int:chain> 
	
	<int:chain id="fundsout.blacklistRepeatcreateActivity" input-channel="FUNDSOUT.BLACKLISTRESEND.ACTIVITY.LOG" output-channel="FUNDSOUT.BLACKLISTRESEND.RESPONSE">
		<int:service-activator id="fundsout.blacklistResendActivityBuilder" ref="blacklistResendActivityBuilder"  method="buildActivityLogs" />
		<int:service-activator id="fundsout.blacklistResendSetServiceStatus" ref="fundsOutDBService"  method="updateServiceStatusIntoPaymetOut" />
		<int:service-activator id="fundsout.blacklistResendActivity"  method="createPaymentOutActivity" >
	    <bean class="com.currenciesdirect.gtg.compliance.compliancesrv.dbport.ActivityDBServiceImpl" />
	    </int:service-activator>
		<int:service-activator id="fundsout.kafkaTxExecutor" ref="kafkaTxExecutorServiceImpl" method="executeKafkaTx" />
	    <int:filter discard-channel="FUNDSOUT.BLACKLIST_RESEND.ERROR.CHANNEL" expression="!payload.isFailed()" />
	</int:chain>
	
	<int:chain input-channel="FUNDSOUT.BLACKLISTRESEND.RESPONSE" output-channel="nullChannel">
	<int:service-activator id="fundsout.BLACKLISTRESEND.RES" ref="blacklistResendResponse" method="process" />
	<int:filter discard-channel="FUNDSOUT.BLACKLIST_RESEND.ERROR.CHANNEL"
			expression="!payload.isFailed()" />
	</int:chain>
	
	
	<int:chain
		id="fundsout.blacklistResendErrorChain"
		input-channel="FUNDSOUT.BLACKLIST_RESEND.ERROR.CHANNEL"
		output-channel="nullChannel">
		<int:service-activator 
			id="fundsout.resend.blacklist"
			ref="blacklist.resend.errorhandler"
			method="logError" />	
	</int:chain>
	
	<bean id="fundsout.MsgValidator" class="com.currenciesdirect.gtg.compliance.compliancesrv.core.validator.fundsout.MessageValidator"/>
	<bean id="blacklistResendResponse" class="com.currenciesdirect.gtg.compliance.compliancesrv.core.blacklist.PrepareBlacklistResendResponse"/>
	<bean id="blacklistResendActivityBuilder" class="com.currenciesdirect.gtg.compliance.compliancesrv.core.blacklist.ResendActivityLogBuilder"/>
	<bean id="eventDBServiceImpl" class="com.currenciesdirect.gtg.compliance.compliancesrv.dbport.EventDBServiceImpl"/>
	<bean id="fundsout.DataEnrich" class="com.currenciesdirect.gtg.compliance.compliancesrv.core.fundsout.DataEnricher"/>	
	<bean id="fundsout.blacklistTransformer" class="com.currenciesdirect.gtg.compliance.compliancesrv.transformer.fundsout.InternalRuleServiceTransformer"/>	
	<bean id="fundsOutDBService" class="com.currenciesdirect.gtg.compliance.compliancesrv.dbport.FundsOutDBServiceImpl"/>
	<bean id="blacklist.resend.errorhandler" class="com.currenciesdirect.gtg.compliance.compliancesrv.core.blacklist.BlacklistRepeatErrorHandler" />
</beans>
