<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:int="http://www.springframework.org/schema/integration"
	xmlns:int-http="http://www.springframework.org/schema/integration/http"
	xmlns:util="http://www.springframework.org/schema/util"
	xmlns:task="http://www.springframework.org/schema/task"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd 
http://www.springframework.org/schema/integration 
http://www.springframework.org/schema/integration/spring-integration.xsd 
http://www.springframework.org/schema/task 
http://www.springframework.org/schema/task/spring-task.xsd 
http://www.springframework.org/schema/context 
http://www.springframework.org/schema/context/spring-context.xsd 
http://www.springframework.org/schema/integration/http
http://www.springframework.org/schema/integration/http/spring-integration-http.xsd
http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd">
	

	<int:channel id="FUNDSOUT.DELETE_OPI.in.channel" />
	<int:channel id="FUNDS_OUT.DELETE.DATA_ENRICHMENT" />
	<int:channel id="FUNDS_OUT.DELETE.DELETE_REQUEST" />
	<int:channel id="FUNDS_OUT.DELETE.TRANFROM.REQUESTS" />
	<int:channel id="FUNDS_OUT.DELETE.RULES.IN" />
	
	<int:channel id="FUNDS_OUT.DELETE.RESPONSE" />


	<int:chain
		id="fundsout.del.initChain"
		input-channel="FUNDSOUT.DELETE_OPI.in.channel">
		<int:header-enricher id="fundsout.del.HeaderEnricher">
			<int:header
				name="correlationID"
				expression="T(java.util.UUID).randomUUID()" />
		</int:header-enricher>
		<int:service-activator
			id="fundsout.del.Validator"
			ref="fundsout.MsgValidator"
			method="process" />
		<int:router
			id="fundsout.del.postValidation"
			expression="payload.getValidationResult()"
			default-output-channel="FUNDS_OUT.DATA_ENRICHMENT">
			<int:mapping
				value="FAIL"
				channel="FUNDS_OUT.RESPONSE" />
			<int:mapping
				value="SUCCESS"
				channel="FUNDS_OUT.DELETE.DATA_ENRICHMENT" />
		</int:router>
	</int:chain>


	<int:chain
		id="fundsout.del.dataEnrichChain"
		input-channel="FUNDS_OUT.DELETE.DATA_ENRICHMENT">
		<int:service-activator
			id="fundsout.del.dataEnrich"
			ref="fundsout.dataEnricher"
			method="enrichData" />
		<int:service-activator
			id="fundsout.del.DataValidation"
			ref="fundsout.DataValidator"
			method="process" />	
		<int:router
			id="fundsout.del.postDataValidation"
			expression="payload.getValidationResult()"
			default-output-channel="FUNDS_OUT.DELETE.DELETE_REQUEST">
			<int:mapping
				value="FAIL"
				channel="FUNDS_OUT.RESPONSE" />
			<int:mapping
				value="SUCCESS"
				channel="FUNDS_OUT.DELETE.DELETE_REQUEST" />
		</int:router>
	</int:chain>

	<int:chain
		id="fundsout.del.dbChain"
		input-channel="FUNDS_OUT.DELETE.DELETE_REQUEST"
		output-channel="FUNDS_OUT.DELETE.TRANFROM.REQUESTS">
		<int:service-activator
			id="fundsout.del.saveRequest"
			ref="fundsOutDBService"
			method="saveFundsOutDeleteRequest" />
		<int:service-activator
			id="fundsout.del.createEvent"
			ref="eventDBServiceImpl"
			method="createEvent" />	
			<int:filter  discard-channel="FUNDS_OUT.ERROR.CHANNEL" expression="!payload.isFailed()" />
	</int:chain>
	
	<int:chain
		id="fundsout.del.transformations"
		input-channel="FUNDS_OUT.DELETE.TRANFROM.REQUESTS"
		output-channel="FUNDS_OUT.DELETE.RULES.IN">
		<int:transformer
			id="fundsout.del.trans.customChecks"
			ref="fundsout.customChecksTransformer"
			method="transformRequest"
		/>
		<int:filter  discard-channel="FUNDS_OUT.ERROR.CHANNEL" expression="!payload.isFailed()" />
		<int:service-activator
			id="fundsout.del.CustomChecks"
			method="sendRequest">
			<bean
				class="com.currenciesdirect.gtg.compliance.compliancesrv.util.HttpClientPool"
				p:urlBase="#{providerProperties.getEndPointUrl('VELOCITY_PERFORM_CHECK')}"
				p:method="POST"
				p:serviceType="#{T(com.currenciesdirect.gtg.compliance.compliancesrv.msg.ServiceTypeEnum).CUSTOM_CHECKS_SERVICE}"
				p:requestClass="com.currenciesdirect.gtg.compliance.commons.domain.customchecks.request.CustomChecksRequest"
				p:responseClass="com.currenciesdirect.gtg.compliance.commons.domain.customchecks.response.CustomCheckResponse" />
		</int:service-activator>
		<int:transformer
			id="fundsout.del.customChecksTransformer"
			ref="fundsout.customChecksTransformer"
			method="transformResponse" />
			<int:filter  discard-channel="FUNDS_OUT.ERROR.CHANNEL" expression="!payload.isFailed()" />
	</int:chain>
	
	<int:chain
		id="fundsout.del.RuleServiceChain"
		input-channel="FUNDS_OUT.DELETE.RULES.IN"
		output-channel="FUNDS_OUT.RESPONSE">
		<!-- <int:service-activator
			id="fundsout.del.SAVE.EVENT.SERVICE"
			ref="eventDBServiceImpl"
			method="createEventSerivceLog">
		</int:service-activator> -->
		<int:service-activator
			id="fundsout.del.Exec.Rules"
			ref="fundsout.RulesService"
			method="process">
		</int:service-activator>
		<int:service-activator
			id="fundsout.del.updateStatus"
			ref="fundsOutDBService"
			method="updatePaymentOutStatus" />
			
		<int:transformer
			ref="fundsout.transactionMonitoringTransformer"
			method="transformRequest" />
		<int:service-activator
			id="fundsout.TRANSACTIONMONITORING"
			method="sendRequest">
			<bean
				class="com.currenciesdirect.gtg.compliance.compliancesrv.util.HttpClientPool"
				p:urlBase="#{providerProperties.getEndPointUrl('TRANSACTION_MONITORING_FUNDSOUT_SERVICE')}"
				p:method="POST"
				p:serviceType="#{T(com.currenciesdirect.gtg.compliance.compliancesrv.msg.ServiceTypeEnum).TRANSACTION_MONITORING_SERVICE}"
				p:requestClass="com.currenciesdirect.gtg.compliance.commons.domain.transactionmonitoring.TransactionMonitoringPaymentsOutRequest"
				p:responseClass="com.currenciesdirect.gtg.compliance.commons.domain.transactionmonitoring.TransactionMonitoringPaymentOutResponse" />
		</int:service-activator>
		<int:transformer
			ref="fundsout.transactionMonitoringTransformer"
			method="transformResponse" />
			
		<int:service-activator
			id="fundsout.del.SAVE.EVENT.SERVICE"
			ref="eventDBServiceImpl"
			method="createEventSerivceLog">
		</int:service-activator>
		
		<int:service-activator
			ref="fundsOutDBService"
			method="updateAccountIntuitionClientRiskLevel">
		</int:service-activator>
		<int:service-activator id="fundsout.kafkaTxExecutor" ref="kafkaTxExecutorServiceImpl" method="executeKafkaTx" />
			
			<int:filter  discard-channel="FUNDS_OUT.ERROR.CHANNEL" expression="!payload.isFailed()" />
	</int:chain>
</beans>