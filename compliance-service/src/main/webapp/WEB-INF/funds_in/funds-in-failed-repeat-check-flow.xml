<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:int="http://www.springframework.org/schema/integration"
	xmlns:int-http="http://www.springframework.org/schema/integration/http"
	xmlns:util="http://www.springframework.org/schema/util"
	xmlns:task="http://www.springframework.org/schema/task"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd 
http://www.springframework.org/schema/integration 
http://www.springframework.org/schema/integration/spring-integration.xsd 
http://www.springframework.org/schema/task 
http://www.springframework.org/schema/task/spring-task.xsd 
http://www.springframework.org/schema/context 
http://www.springframework.org/schema/context/spring-context.xsd 
http://www.springframework.org/schema/integration/http
http://www.springframework.org/schema/integration/http/spring-integration-http.xsd
http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd">

	<context:component-scan base-package="com.currenciesdirect.gtg.compliance.compliancesrv" />
	
	<int:channel id="FUNDSIN.RECHECK_FAILURES.in.channel" />
	<int:channel id="FUNDS_IN.RECHECK_FAILURES.DATA_ENRICHMENT" />
	<int:channel id="FUNDS_IN.RECHECK_FAILURES.PERFORM.FRAUGSTER" />
	<int:channel id="FUNDS.IN.RECHECK_FAILURES.FRAUGSTER.TRANSFORMER" />
	<int:channel id="FUNDS_IN.RECHECK_FAILURES.PERFORM.SANCTION" />
	<int:channel id="FUNDS.IN.RECHECK_FAILURES.SANCTION.TRANSFORMER" />
	<int:channel id="FUNDS_IN.RECHECK_FAILURES.RULES.IN" />
	<int:channel id="FUNDS_IN.RESPONSE" />
	<int:channel id="FUNDS_IN.RECHECK.SAVETOBROADCAST.IN" />
	<int:channel id="FUNDS_IN.RECHECK_FAILURES.PERFORM.InternalRuleService" />
	<int:channel id="FUNDS.IN.RECHECK_FAILURES.INTERNALRULE.TRANSFORMER" />
	<int:channel id="FUNDS_IN.RECHECK_FAILURES.PERFORM.CustomCheck" />
	<int:channel id="FUNDS.IN.RECHECK_FAILURES.CUSTOMCHECK.TRANSFORMER" /> 
	<int:channel id="FUNDS_IN.RECHECK.TRANSACTIONMONITORINGSTATUS" />
	
	<int:chain
		id="fundsin.recheck.initChain"
		input-channel="FUNDSIN.RECHECK_FAILURES.in.channel"
		output-channel="FUNDS_IN.RECHECK_FAILURES.DATA_ENRICHMENT">
		<int:header-enricher id="fundsin.recheck.HeaderEnricher">
			<int:header
				name="correlationID"
				expression="T(java.util.UUID).randomUUID()" />
		</int:header-enricher>		
	</int:chain>
	
	<int:chain
		id="fundsin.recheck.dataEnrichChain"
		input-channel="FUNDS_IN.RECHECK_FAILURES.DATA_ENRICHMENT"
		output-channel="FUNDS_IN.RECHECK_FAILURES.PERFORM.InternalRuleService">
		<int:service-activator
			id="fundsin.recheck.UPDATERECHECKSTATUS.INPROGRESS"
			ref="fundsin.recheck.RecheckDBServiceImpl"
			method="updateFailedRecheckStatusToInProgress"/>
		<int:service-activator
			id="fundsin.recheck.dataEnrich"
			ref="fundsin.recheckdataEnricher"
			method="enrichDataForFailedFundsIn" />
		<int:service-activator
			id="fundsin.recheck.createEvent"
			ref="eventDBServiceImpl" 
			method="createEvent" />
	</int:chain>
	
	<int:chain
		id="fundsin.internalRule.decideCheck"
		input-channel="FUNDS_IN.RECHECK_FAILURES.PERFORM.InternalRuleService">
		
		<int:router
			default-output-channel="FUNDS_IN.ERROR.CHANNEL"
			resolution-required="false"
			expression="payload.isInternalRuleServiceEligible()">
			<int:mapping
				value="true"
				channel="FUNDS.IN.RECHECK_FAILURES.INTERNALRULE.TRANSFORMER" />
			<int:mapping
				value="false"
				channel="FUNDS_IN.RECHECK_FAILURES.PERFORM.CustomCheck" />
		</int:router>
	</int:chain>
	
	<int:chain
		id="fundsin.internalRuleRepeatCheck"
		input-channel="FUNDS.IN.RECHECK_FAILURES.INTERNALRULE.TRANSFORMER"
		output-channel="FUNDS_IN.RECHECK_FAILURES.PERFORM.CustomCheck">
	
		<int:transformer
			id="fundsin.recheck.trans.internalRule"
			ref="fundsin.internalRuleBulkRecheckTransformer"
			method="transformRequest" />
		<int:service-activator
			id="fundsin.InternalRules"
			method="sendRequest">
			<bean
				class="com.currenciesdirect.gtg.compliance.compliancesrv.util.HttpClientPool"
				p:urlBase="#{providerProperties.getEndPointUrl('INTERNAL_RULE_SERVICE')}"
				p:method="POST"
				p:serviceType="#{T(com.currenciesdirect.gtg.compliance.compliancesrv.msg.ServiceTypeEnum).INTERNAL_RULE_SERVICE}"
				p:requestClass="com.currenciesdirect.gtg.compliance.commons.domain.internalruleservice.InternalServiceRequest"
				p:responseClass="com.currenciesdirect.gtg.compliance.commons.domain.internalruleservice.InternalServiceResponse" />
		</int:service-activator>
		<int:transformer
			ref="fundsin.internalRuleBulkRecheckTransformer"
			method="transformResponse" />
		
		<int:filter  discard-channel="FUNDS_IN.ERROR.CHANNEL" expression="!payload.isFailed()" />
	</int:chain>
	
	<int:chain
		id="fundsin.customCheck.decideCheck"
		input-channel="FUNDS_IN.RECHECK_FAILURES.PERFORM.CustomCheck">
		
		<int:router
			default-output-channel="FUNDS_IN.ERROR.CHANNEL"
			resolution-required="false"
			expression="payload.isCustomCheckEligible()">
			<int:mapping
				value="true"
				channel="FUNDS.IN.RECHECK_FAILURES.CUSTOMCHECK.TRANSFORMER" />
			<int:mapping
				value="false"
				channel="FUNDS_IN.RECHECK_FAILURES.PERFORM.FRAUGSTER" />
		</int:router>
	</int:chain>
	
	<int:chain
		id="fundsin.customCheckRepeatCheck"
		input-channel="FUNDS.IN.RECHECK_FAILURES.CUSTOMCHECK.TRANSFORMER"
		output-channel="FUNDS_IN.RECHECK_FAILURES.PERFORM.FRAUGSTER">
	
		<int:transformer
			id="fundsin.recheck.trans.customCheck"
			ref="fundsin.customCheckBulkRecheckTransformer"
			method="transformRequest" />
		<int:service-activator
			id="fundsin.recheck.CustomChecks"
			method="sendRequest">
			<bean
				class="com.currenciesdirect.gtg.compliance.compliancesrv.util.HttpClientPool"
				p:urlBase="#{providerProperties.getEndPointUrl('VELOCITY_PERFORM_CHECK')}"
				p:method="POST"
				p:serviceType="#{T(com.currenciesdirect.gtg.compliance.compliancesrv.msg.ServiceTypeEnum).CUSTOM_CHECKS_SERVICE}"
				p:requestClass="com.currenciesdirect.gtg.compliance.commons.domain.customchecks.request.CustomChecksRequest"
				p:responseClass="com.currenciesdirect.gtg.compliance.commons.domain.customchecks.response.CustomCheckResponse" />
		</int:service-activator>
		<int:transformer
			ref="fundsin.customCheckBulkRecheckTransformer"
			method="transformResponse" />
		
		<int:filter  discard-channel="FUNDS_IN.ERROR.CHANNEL" expression="!payload.isFailed()" />
	</int:chain>
	
	<int:chain
		id="fundsin.fraugster.decideCheck"
		input-channel="FUNDS_IN.RECHECK_FAILURES.PERFORM.FRAUGSTER">
		
		<int:router
			default-output-channel="FUNDS_IN.ERROR.CHANNEL"
			resolution-required="false"
			expression="payload.isFraugsterEligible()">
			<int:mapping
				value="true"
				channel="FUNDS.IN.RECHECK_FAILURES.FRAUGSTER.TRANSFORMER" />
			<int:mapping
				value="false"
				channel="FUNDS_IN.RECHECK_FAILURES.PERFORM.SANCTION" />
		</int:router>
	</int:chain>
	
	<int:chain
		id="fundsin.fraugsterRepeatCheck"
		input-channel="FUNDS.IN.RECHECK_FAILURES.FRAUGSTER.TRANSFORMER"
		output-channel="FUNDS_IN.RECHECK_FAILURES.PERFORM.SANCTION">
	
		<int:transformer
			id="fundsin.recheck.trans.fraugster"
			ref="fundsin.fraugsterBulkRecheckTransformer"
			method="transformRequest" />
		<int:service-activator
			id="fundsin.recheck.FRAUDCHECK"
			method="sendRequest">
			<bean
				class="com.currenciesdirect.gtg.compliance.compliancesrv.util.HttpClientPool"
				p:urlBase="#{providerProperties.getEndPointUrl('FRAUGSTER_FUNDSIN_SERVICE')}"
				p:method="POST"
				p:serviceType="#{T(com.currenciesdirect.gtg.compliance.compliancesrv.msg.ServiceTypeEnum).FRAUGSTER_SERVICE}"
				p:requestClass="com.currenciesdirect.gtg.compliance.commons.domain.fraugster.FraugsterPaymentsInRequest"
				p:responseClass="com.currenciesdirect.gtg.compliance.commons.domain.fraugster.FraugsterPaymentsInResponse" />
		</int:service-activator>
		<int:transformer
			ref="fundsin.fraugsterBulkRecheckTransformer"
			method="transformResponse" />
		
		<int:filter  discard-channel="FUNDS_IN.ERROR.CHANNEL" expression="!payload.isFailed()" />
	</int:chain>
	
	<int:chain
		id="fundsin.recheck.sanction.decideCheck"
		input-channel="FUNDS_IN.RECHECK_FAILURES.PERFORM.SANCTION">
		
		<int:router
			default-output-channel="FUNDS_IN.ERROR.CHANNEL"
			resolution-required="false"
			expression="payload.isSanctionEligible()">
			<int:mapping
				value="true"
				channel="FUNDS.IN.RECHECK_FAILURES.SANCTION.TRANSFORMER" />
			<int:mapping
				value="false"
				channel="FUNDS_IN.RECHECK_FAILURES.RULES.IN" />
		</int:router>
	</int:chain>
	
	<int:chain
		id="fundsin.recheck.sanctionRepeatCheck"
		input-channel="FUNDS.IN.RECHECK_FAILURES.SANCTION.TRANSFORMER"
		output-channel="FUNDS_IN.RECHECK_FAILURES.RULES.IN">
	
		<int:transformer
			id="fundsin.recheck.trans.sanction"
			ref="fundsin.sanctionBulkRecheckTransformer"
			method="transformRequest"
		/>
		<int:service-activator
			id="fundsin.recheck.SANCTION"
			method="sendRequest">
			<bean
				class="com.currenciesdirect.gtg.compliance.compliancesrv.util.HttpClientPool"
				p:urlBase="#{providerProperties.getEndPointUrl('SANCTION_SERVICE')}"
				p:method="POST"
				p:serviceType="#{T(com.currenciesdirect.gtg.compliance.compliancesrv.msg.ServiceTypeEnum).SANCTION_SERVICE}"
				p:requestClass="com.currenciesdirect.gtg.compliance.commons.domain.sanction.ContactRegSanctionRequest"
				p:responseClass="com.currenciesdirect.gtg.compliance.commons.domain.sanction.SanctionResponse" />
		</int:service-activator>
		<int:transformer
			ref="fundsin.sanctionBulkRecheckTransformer"
			method="transformResponse" />
		
		<int:filter  discard-channel="FUNDS_IN.ERROR.CHANNEL" expression="!payload.isFailed()" />
	</int:chain>
	
	<int:chain
		id="fundsin.recheck.RuleServiceChain"
		input-channel="FUNDS_IN.RECHECK_FAILURES.RULES.IN"
		output-channel="FUNDS_IN.RECHECK.INTUITIONSTATUS">
		
		<int:transformer
			ref="fundsin.transactionMonitoringTransformer"
			method="transformRequest" />
		<int:service-activator
			id="fundsin.recheck.TRANSACTIONMONITORING"
			method="sendRequest">
			<bean
				class="com.currenciesdirect.gtg.compliance.compliancesrv.util.HttpClientPool"
				p:urlBase="#{providerProperties.getEndPointUrl('TRANSACTION_MONITORING_FUNDSIN_SERVICE')}"
				p:method="POST"
				p:serviceType="#{T(com.currenciesdirect.gtg.compliance.compliancesrv.msg.ServiceTypeEnum).TRANSACTION_MONITORING_SERVICE}"
				p:requestClass="com.currenciesdirect.gtg.compliance.commons.domain.transactionmonitoring.TransactionMonitoringPaymentsInRequest"
				p:responseClass="com.currenciesdirect.gtg.compliance.commons.domain.transactionmonitoring.TransactionMonitoringPaymentInResponse" />
		</int:service-activator>
		<int:transformer
			ref="fundsin.transactionMonitoringTransformer"
			method="transformResponse" />	
			
		<!-- <int:service-activator
			ref="eventDBServiceImpl"
			method="createTransactionMonitoringEventSerivceLog">
		</int:service-activator> -->
		
		<int:service-activator
			id="fundsin.recheck.SAVE.EVENT.SERVICE"
			ref="eventDBServiceImpl"
			method="createEventSerivceLogForServiceFailedFundsIn">
		</int:service-activator>
		
		<int:service-activator
			id="fundsin.recheck.Exec.Rules"
			ref="fundsin.RulesService"
			method="process">
		</int:service-activator>
		
		<int:service-activator
			id="fundsin.recheck.updateStatus"
			ref="fundsInDBService"
			method="updatePaymentInStatus" />
			
		<int:service-activator
			ref="fundsInDBService"
			method="updateAccountIntuitionClientRiskLevel">
		</int:service-activator>
		<int:service-activator
			id="fundsin.kafkaTxExecutor"
			ref="kafkaTxExecutorServiceImpl"
			method="executeKafkaTx" />
		
		<int:service-activator
			id="fundsin.recheck.activitylog"
			ref="activityDBServiceImpl"
			method="createServiceFailedRecheckPaymentInActivity" />
		<int:filter  discard-channel="FUNDS_IN.ERROR.CHANNEL" expression="!payload.isFailed()" />
	</int:chain>
	
	<int:chain
		input-channel="FUNDS_IN.RECHECK.INTUITIONSTATUS"> <!-- AT-4715 -->
		<int:router
			default-output-channel="FUNDS_IN.RECHECK.SAVETOBROADCAST.IN"
			resolution-required="false"
			expression="payload.isIntuitionStatusHold()">
			<int:mapping
				value="true"
				channel="FUNDS_IN.RECHECK.TRANSACTIONMONITORINGSTATUS" />
			<int:mapping
				value="false"
				channel="FUNDS_IN.RECHECK.SAVETOBROADCAST.IN" />
		</int:router>
	</int:chain>
	
	<int:chain input-channel="FUNDS_IN.RECHECK.TRANSACTIONMONITORINGSTATUS"
		output-channel="FUNDS_IN.RECHECK.SAVETOBROADCAST.IN"> <!-- AT-4715 -->
		<int:service-activator
			id="fundsin.recheck.TRANSACTIONMONITORINGSTATUS"
			method="sendRequest">
			<bean
				class="com.currenciesdirect.gtg.compliance.compliancesrv.util.HttpClientPool"
				p:urlBase="#{providerProperties.getEndPointUrl('TRANSACTION_MONITORING_FUNDSIN_SERVICE')}"
				p:method="POST"
				p:serviceType="#{T(com.currenciesdirect.gtg.compliance.compliancesrv.msg.ServiceTypeEnum).TRANSACTION_MONITORING_SERVICE}"
				p:requestClass="com.currenciesdirect.gtg.compliance.commons.domain.transactionmonitoring.TransactionMonitoringPaymentsInRequest"
				p:responseClass="com.currenciesdirect.gtg.compliance.commons.domain.transactionmonitoring.TransactionMonitoringPaymentInResponse" />
		</int:service-activator>
	</int:chain>
	
	<int:chain
		id="fundsInRecheckBroadCastChain"
		input-channel="FUNDS_IN.RECHECK.SAVETOBROADCAST.IN"
		output-channel="FUNDS_IN.RESPONSE">
		
		<int:transformer
			id="fundsin.recheck.BROADCAST.TRANSFORM"
			ref="fundsin.recheck.saveToBroadCastTransformer"
			method="transformRequestForFailedFundIn" />
		<int:service-activator
			id="fundsin.recheck.SAVETOBROADCAST"
			ref="fundsin.recheck.BroadCastQueueDBServiceImpl"
			method="saveIntoBroadcastQueue">
		</int:service-activator>
		<int:service-activator
			id="fundsin.recheck.UPDATERECHECKSTATUS.DONE"
			ref="fundsin.recheck.RecheckDBServiceImpl"
			method="updateFailedFundsInStatusToDone">
		</int:service-activator>
		<int:filter discard-channel="FUNDS_IN.ERROR.CHANNEL" expression="!payload.isFailed()" />
	</int:chain>
	
	

	<bean id="fundsin.recheckdataEnricher" class="com.currenciesdirect.gtg.compliance.compliancesrv.core.fundsin.DataEnricher" />
	<bean id="fundsin.fraugsterBulkRecheckTransformer" class="com.currenciesdirect.gtg.compliance.compliancesrv.transformer.fundsin.FraugsterBulkRecheckTransformer" />
	<bean id="fundsin.sanctionBulkRecheckTransformer" class="com.currenciesdirect.gtg.compliance.compliancesrv.transformer.fundsin.SanctionBulkRecheckTransformer" />
	<bean id="fundsin.internalRuleBulkRecheckTransformer" class="com.currenciesdirect.gtg.compliance.compliancesrv.transformer.fundsin.InternalRuleBulkRecheckTransformer" />
	<bean id="fundsin.customCheckBulkRecheckTransformer" class="com.currenciesdirect.gtg.compliance.compliancesrv.transformer.fundsin.CustomCheckBulkRecheckTransformer" />
	<bean id="fundsin.recheck.BroadCastQueueDBServiceImpl" class="com.currenciesdirect.gtg.compliance.compliancesrv.dbport.BroadCastQueueDBServiceImpl" />
	<bean id="fundsin.recheck.saveToBroadCastTransformer" class="com.currenciesdirect.gtg.compliance.compliancesrv.transformer.mqbroadcast.SaveToBroadCastTransformer" />
	<bean id="fundsin.recheck.RecheckDBServiceImpl" class="com.currenciesdirect.gtg.compliance.compliancesrv.dbport.RecheckDBServiceImpl" />
</beans>